{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 400,
    "title": "Trend of COVID-19 Deaths Over Time",
    "data": {"url": "https://raw.githubusercontent.com/owid/covid-19-data/master/public/data/latest/owid-covid-latest.csv"},
    "transform": [
      {"filter": "datum.continent != \"\""},
      {"filter": "datum.human_development_index != \"\""}
    ],
    "mark": "circle",
    "encoding": {
      "x": {"field": "human_development_index", "type": "quantitative"},
      "y": {"field": "total_deaths_per_million", "type": "quantitative", "axis": {"tickCount": 6}, "scale": {"type": "symlog"}},
      "color": {"field": "continent", "type": "nominal"},
      "size": {"field": "population", "type": "quantitative", "scale": {
 "type": "threshold",
 "domain": [1000000, 10000000, 50000000, 100000000, 500000000],
 "range": [10, 50, 150, 200, 300, 400]
 },
 "legend": {"format": ".1s"}
},
      "tooltip": [
        {"field": "location", "type": "nominal", "title": "Country"},
        {"field": "total_deaths", "type": "quantitative", "title": "Total Deaths"}
      ]
    }
  }
  